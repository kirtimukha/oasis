{
  "useEditorConfig": false,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": true,
  "semi": true,
  "trailingComma": "all",
  "objectCurlySpacing": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "vueIndentScriptAndStyle": true,
  "endOfLine": "auto",
  "jsxSingleQuote": true,
  "proseWrap": "preserve",
  "singleQuote": true,
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "printWidth": 200
      }
    }
  ],
  "disableLanguages": [
    "md",
    "txt",
    "xlsx"
  ]
}